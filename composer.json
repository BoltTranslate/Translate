{
    "name": "animal/translate",
    "description": "This extension provides translation of records. Has support for repeaters, templatefields and extension fieldtypes.",
    "type": "bolt-extension",
    "require": {
        "bolt/bolt": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "keywords": [
        "Translation",
        "Frontend",
        "Language",
        "Languages",
        "Localization",
        "Localisation",
        "Multilanguage",
        "Content",
        "i18n"
    ],
    "authors": [
        {
            "name": "Animal Design OG",
            "email": "studio@animal.at"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Animal\\Translate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Animal\\Translate\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-icon": "icon-language.png",
        "bolt-class": "Bolt\\Extension\\Animal\\Translate\\TranslateExtension"
    }
}
